{{!
This module is responsible for rendering the header
which includes the logo, login, and need help buttons
also the log in status
}}
<div id="header">
  <div class="container-fluid">
    <div class="row">
      <div id="header-logo"></div>
      <div id="login-wrapper" class="header-btn-group logininfo col-lg-4 col-xs-12">
        <div class="btn btn-header" id="btn-need-help" >{{{output.help_feedback_link}}}</div>
        <button class="btn btn-header" id="btn-login-status">{{# str }} loggedinnot {{/ str }}</button>
        <a href={{loginlink}}><button class="btn btn-header" id="btn-login">{{# str }} login {{/ str }}</button></a>
      </div>
    </div>
      {{{ output.alert_banner }}}
  </div>
</div>
