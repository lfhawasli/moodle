<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="report/emaillog/db" VERSION="20160112" COMMENT="XMLDB file for Moodle report/emaillog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="report_emaillog" COMMENT="Log table for sent emails through forums">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="post" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="recipient_email" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timestamp" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="post_index" UNIQUE="false" FIELDS="post"/>
        <INDEX NAME="timestamp_index" UNIQUE="false" FIELDS="timestamp"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>