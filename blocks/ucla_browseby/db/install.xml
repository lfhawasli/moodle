<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/ucla_browseby/db" VERSION="20181003" COMMENT="XMLDB file for Moodle blocks/ucla_browseby"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="ucla_browseall_instrinfo" COMMENT="Tables maintains a cache for the results from ccle_getinstrinfo.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="uid" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="profcode" TYPE="char" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="firstname" TYPE="char" LENGTH="25" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="middlename" TYPE="char" LENGTH="25" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="lastname" TYPE="char" LENGTH="25" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="suffix" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="subjarea" TYPE="char" LENGTH="7" NOTNULL="false" SEQUENCE="false" COMMENT="This field is not provided by the stored procedure, and needs to be added into the data manually."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="termsrsuid" UNIQUE="true" FIELDS="term, srs, uid"/>
        <INDEX NAME="termsubjarea" UNIQUE="false" FIELDS="term, subjarea"/>
        <INDEX NAME="uid" UNIQUE="false" FIELDS="uid"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="ucla_browseall_classinfo" COMMENT="Classes information cached from the Registrar via ccle_getcoursesall.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false" COMMENT="9 digit srs"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false" COMMENT="This field is not provided by the stored procedure, it is added manually before adding data to the table."/>
        <FIELD NAME="subjarea" TYPE="char" LENGTH="7" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="course" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="session" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="section" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursetitlelong" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sectnoteseq" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="activitytype" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="sect_no" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="catlg_no" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sect_enrl_stat_cd" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="ses_grp_cd" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="termsrs" UNIQUE="true" FIELDS="term, srs"/>
        <INDEX NAME="subjareaindex" UNIQUE="false" FIELDS="subjarea"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>