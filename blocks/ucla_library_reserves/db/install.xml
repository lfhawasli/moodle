<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/ucla_library_reserves/db" VERSION="20170928" COMMENT="XMLDB file for Moodle blocks/ucla_library_reserves"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="ucla_library_reserves" COMMENT="Library reserves Database">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="course_number" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="course_name" TYPE="char" LENGTH="40" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="department_code" TYPE="char" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="department_name" TYPE="char" LENGTH="40" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="url" TYPE="char" LENGTH="400" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="quarter" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="courseid" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="term_srs" UNIQUE="false" FIELDS="quarter, srs"/>
        <INDEX NAME="term_course" UNIQUE="false" FIELDS="quarter, department_code, course_number"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>