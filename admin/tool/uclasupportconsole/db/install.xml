      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="uniqtermsrs" TYPE="unique" FIELDS="term, srs"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>