<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="admin/tool/uclasupportconsole/db" VERSION="20150511" COMMENT="XMLDB file for Moodle admin/tool/uclasupportconsole"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="uclaieiclasses" COMMENT="List of classes that were charged an IEI fee.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
