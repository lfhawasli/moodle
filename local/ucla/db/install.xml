<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/ucla/db" VERSION="20141013" COMMENT="XMLDB file for Moodle local/ucla"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="ucla_reg_subjectarea" COMMENT="Holds information for subject areas obtained by cis_subjectareagetall">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="subjarea" TYPE="char" LENGTH="7" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="subj_area_full" TYPE="char" LENGTH="60" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="home" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="modified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="dexs" UNIQUE="true" FIELDS="subjarea"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="ucla_reg_classinfo" COMMENT="ucla_reg_classinfo table retrofitted from MySQL">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="subj_area" TYPE="char" LENGTH="7" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursenum" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sectnum" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="crsidx" TYPE="char" LENGTH="8" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="classidx" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="secidx" TYPE="char" LENGTH="6" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="secttype" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="srs" TYPE="char" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="term" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="division" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="acttype" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="coursetitle" TYPE="char" LENGTH="254" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="sectiontitle" TYPE="char" LENGTH="240" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="enrolstat" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="session_group" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="session" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="crs_desc" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="crs_summary" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="Term specific class description"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="mdl_uclaregclas_tersrs_uix" TYPE="unique" FIELDS="term, srs"/>
        <KEY NAME="mdl_uclaregclas_tersubcrss_uix" TYPE="unique" FIELDS="term, subj_area, crsidx, secidx"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="mdl_uclaregclas_subjarea_ix" UNIQUE="false" FIELDS="subj_area"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="ucla_rolemapping" COMMENT="Table to map pseudorole from registrar to moodle roles">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="pseudo_role" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="description" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="describes the rule that translates to a moodle role"/>
        <FIELD NAME="moodle_roleid" TYPE="int" LENGTH="2" NOTNULL="true" SEQUENCE="false" COMMENT="Id for the role whose shortname is in mdl_role table"/>
        <FIELD NAME="subject_area" TYPE="char" LENGTH="50" NOTNULL="true" DEFAULT="*SYSTEM*" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="ucla_reg_division" COMMENT="ucla_reg_division table contains list of UCLA Divisions from Registrar, filled by hand first time.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="code" TYPE="char" LENGTH="2" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="fullname" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="home" TYPE="int" LENGTH="4" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="timestamp" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="code" TYPE="unique" FIELDS="code"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="ccle_roster_class_cache" COMMENT="Cache table for the stored procedure ccle_roster_class">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="param_term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="param_srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="expires_on" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="term_cd" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="stu_id" TYPE="char" LENGTH="9" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="full_name_person" TYPE="char" LENGTH="70" NOTNULL="false" SEQUENCE="false" COMMENT="Name of the student in “LAST, FIRST MIDDLE” format"/>
        <FIELD NAME="enrl_stat_cd" TYPE="char" LENGTH="1" NOTNULL="false" SEQUENCE="false" COMMENT="Enrol status. For regular students: E – enrolled D – Dropped W – Wait List H – Held For UNEX students: A – Approved C – Cancelled P – Pending"/>
        <FIELD NAME="ss_email_addr" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="bolid" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" COMMENT="Bruin Online ID, usually same as UCLA Login"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="param_index" UNIQUE="false" FIELDS="param_term, param_srs, expires_on"/>
        <INDEX NAME="student_uid_lookup" UNIQUE="false" FIELDS="param_term, param_srs, stu_id"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="ccle_get_primary_srs_cache" COMMENT="Cache table for the stored procedure ccle_get_primary_srs">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="param_term" TYPE="char" LENGTH="3" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="param_srs" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="expires_on" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="srs_crs_no" TYPE="char" LENGTH="9" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="param_index" UNIQUE="false" FIELDS="param_term, param_srs, expires_on"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
